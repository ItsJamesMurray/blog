{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/sitemap",
        "function": "sitemap"
      },
      {
        "source": "**",
        "function": "ssr"
      }
    ]
  }
}
